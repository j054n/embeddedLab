#NET "sw[4]" LOC = "L14"; # Bank = 1, Pin name = IP, Type = INPUT, Sch name = SW4
#NET "sw[5]" LOC = "L13"; # Bank = 1, Pin name = IP, Type = INPUT, Sch name = SW5
